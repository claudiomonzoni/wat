<Footer />

<style lang="scss">
  @import "../estilos/_variables.scss";
  #logo{
    
    width: 80%;
    height: 80%;
    @include cel {
      width: 100%;
      height: 100%;
    }
  }

  .hero {
    display: flex;
    padding: 2rem 0;
    height: 70vh;
    @include cel {
      flex-wrap: wrap;
    }
    .izq,
    .der {
      flex: 1 1 auto;
      display: inherit;
      justify-content: center;
      align-items: center;
    }

    .conte {
      width: 70%;
    }

    img {
      width: 80%;
    }
    h2 {
      color: $azul;
      font-size: clamp(1.3rem, 2vw, 2rem);
      font-weight: lighter;
    }
  }

  .borde-abajo {
    border-bottom: solid 1px $dorado;
    padding-bottom: $margen;
  }
  .anchoauto {
    width: auto !important;
    margin-bottom: $margen-abajo;
  }
  .pad {
    padding: 0 20%;
  }
  ul {
    padding: $margen 0;
    margin-left: $margen;
    li {
      margin-bottom: 0.6rem;
      list-style-type: disc;
    }
  }
  .raya {
    text-decoration: line-through;
  }
  #riel {
    display: grid;
    grid-auto-flow: column;
    overflow: hidden;
    padding-left: $margen-riel;
  }

  .vagon {
    width: 100vw;

    @include cel {
      width: 85vw;
      .bande {
        flex-wrap: wrap;
      }
      .conte {
        flex: 1 0 100% !important;
        padding: 0 !important;
        &:last-child {
          border-left: none !important;
        }
      }
    }

    h3 {
      color: $negro;
      font-size: clamp(1.1rem, 1.3vw, 1.5rem);
    }
    .bande {
      display: flex;
      gap: 2rem;

      .conte {
        flex: 1;
        padding: 2rem;
        margin-top: 2rem;
        &:last-child {
          border-left: solid 1px $azul;
        }
        h3 {
          color: $dorado;
        }
      }
    }
  }
  #asesorias {
    padding-bottom: $margen-abajo;
    overflow: hidden;
    #riel2 {
      padding-top: $margen;
      display: flex;
    }
    .asesoria {
      display: flex;
      flex: 1 0 60%;
      align-items: center;
      justify-content: center;
      width: 100%;
      @include cel {
        flex-wrap: wrap;
        .conte {
          border-left: none !important;
          border-top: solid 1px $azul;
          font-size: clamp(1.2rem, 2vw, 1.5rem);
          margin: 1rem;
        }
      }
      .num {
        font-size: clamp(5rem, 2.3vw, 6rem);
        font-weight: bold;
        padding: 2rem;
      }
      .conte {
        border-left: solid 1px $azul;
        padding: 2rem;
      }
    }
  }
</style>
<script>
  import gsap from "gsap";

  let tl = gsap.timeline({ repeat: 0 });
  tl.delay(2);

  tl.from(" .init > .ref", {
    stagger: {
      amount: 0.3,
    },
    y: 100,
    opacity: 0,
  });

  tl.from("#logo path", {
    stagger: {
      amount: 2.2,
     
    },
    delay: -0.2,
    x: "random",
    // rotate: random,
    scaleY: 1.5,
    scaleX: 1.5,
    transformOrigin: "top",
    ease: "Power1.easeOut",
    opacity: 0,
    duration: 0.7,
  });

  import { ScrollTrigger } from "gsap/ScrollTrigger";
  gsap.registerPlugin(ScrollTrigger);

  const riel = document.querySelector("#riel");
  const riel2 = document.querySelector("#riel2");
  const vagones = gsap.utils.toArray("#riel .vagon");
  const asesoria = gsap.utils.toArray("#riel2 .asesoria");
  const losvagones = document.querySelectorAll("#riel .vagon");

  let scrollAnima = gsap.to(vagones, {
    xPercent: -100 * (vagones.length - 1),
    ease: "none",
    scrollTrigger: {
      trigger: riel,
      pin: true,
      start: "center center center",
      scrub: 1,
      end: "+=3000",
      snap: {
        snapTo: 1 / (vagones.length - 1),
        duration: 1,
        delay: 0.6,
        ease: "power1.inOut",
      },
      // markers: true,
    },
  });

  let scrollAsesoria = gsap.to(asesoria, {
    xPercent: -100 * (asesoria.length - 1),
    ease: "none",
    scrollTrigger: {
      trigger: riel2,
      pin: true,
      start: "center center center",
      scrub: 1,
      end: "+=3000",
      snap: {
        snapTo: 1 / (asesoria.length - 1),
        duration: 1,
        delay: 0.6,
        ease: "power1.inOut",
      },
      // markers: true,
    },
  });

  losvagones.forEach((vagon) => {
    let h3 = vagon.querySelectorAll("h3");
    let h2 = vagon.querySelectorAll("h2");
    let p = vagon.querySelectorAll("p");
    let conte = vagon.querySelectorAll(".conte");

    gsap.from([h3, p, conte], {
      y: -50,
      opacity: 0,
      stagger: 0.1,
      scrollTrigger: {
        trigger: vagon,
        containerAnimation: scrollAnima,
        start: "left center",
      },
    });
  });

  const proyectos = document.querySelectorAll(".int");
  // For each images with class "animate-image" on page
  proyectos.forEach((el) => {
    let tl = gsap.timeline({
      scrollTrigger: {
        trigger: el,
        start: "left center",
        // toggleActions: "play none none reverse",
      },
    });

    tl.set(el, { transformOrigin: "top center" }).from(el, {
      opacity: 0,
      scale: 0.9,
      y: "+=20",
    });
  });
</script>
